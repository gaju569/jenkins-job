pipeline {

  agent any
  stages {
   
    stage('build') {
      steps {
         echo "building application-1.."
      } 
    }    

    stage('test') {
     steps {
       echo "testing application-1..."
     }
   }
   
   stage('deploy')
      steps {
         echo "deploying application-1..."
       } 
    } 
    
   } 
 
} 
